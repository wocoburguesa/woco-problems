#include <iostream>
#include <vector>
#include <map>
#include <algorithm>
#include <sstream>
#include <math.h>

using namespace std;

int main(){
  int rows, cols;
  while(cin >> rows >> cols && rows, cols){
    vector<string> board, solved;
    string buff;
    while(rows--){
      cin >> buff;
      board.push_back(buff);
      solved.push_back(buff);
    }
    for(int i = 0; i < rows; ++i)
      for(int j = 0; j < cols; ++j)
	if(solved[i][j] != '*')
	  solved = 0;

    for(int i = 0; i < rows; ++i)
      for(int j = 0; j < cols; ++j)
	if(board[i][j] == '*'){
	  if(i > 0 && j > 0){
	    solved[i-1][j-1]++;
	    solved[i-1][j]++;
	    solved[i][j-1]++;
	  }
	  else if(i > 0)
	    solved[i-1][j]++;
	  else
	    solved[i][j-1]++;
	  if(i < rows && j > 0){
	    solved[i+1][j-1]++;
	    solved[i+1][j]++;
	    solved[i][j-1]++;
	  }
	  else if(i < rows)
	    solved[i+1][j]++;
	  else;
	  if(i > 0 && j < cols){
	    solved[i-1][j+1]++;
	    solved[i-1][j]++;
	    solved[i][j+1]++;
	  }
	  else if(i > 0)
	    solved[i-1][j]++
	}
	  solved = 0;

  }
  return 0;
}
